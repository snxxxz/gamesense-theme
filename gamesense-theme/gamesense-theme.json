        "notification.errorBackground": "#ff3333",
        "notification.errorForeground": "#a0a0a0"
    },
    "tokenColors": [
        {
            "scope": ["comment"],
            "settings": {
                "foreground": "#a0a0a0",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["string"],
            "settings": {
                "foreground": "#95b806",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["keyword", "storage"],
            "settings": {
                "foreground": "#95b806",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["function", "method"],
            "settings": {
                "foreground": "#a0a0a0",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["variable", "parameter"],
            "settings": {
                "foreground": "#95b806",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["number", "constant"],
            "settings": {
                "foreground": "#a0a0a0",
                "fontStyle": "bold"
            }
        }
    ]
}
